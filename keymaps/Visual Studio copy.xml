<keymap version="1" name="Visual Studio copy" parent="Visual Studio">
  <action id="CollapseAllRegions">
    <keyboard-shortcut first-keystroke="ctrl alt multiply" />
  </action>
  <action id="CollapseDocComments">
    <keyboard-shortcut first-keystroke="ctrl multiply" second-keystroke="ctrl subtract" />
  </action>
  <action id="CollapseRegion">
    <keyboard-shortcut first-keystroke="shift ctrl alt multiply" />
  </action>
  <action id="ExpandToLevel1">
    <keyboard-shortcut first-keystroke="ctrl multiply" second-keystroke="1" />
    <keyboard-shortcut first-keystroke="ctrl multiply" second-keystroke="numpad1" />
    <keyboard-shortcut first-keystroke="ctrl cancel" second-keystroke="ctrl 1" />
  </action>
  <action id="MoveTabDown">
    <keyboard-shortcut first-keystroke="ctrl #10000ba" second-keystroke="ctrl left" />
  </action>
  <action id="MoveTabRight">
    <keyboard-shortcut first-keystroke="ctrl #10000ba" second-keystroke="ctrl right" />
  </action>
  <action id="ShowBookmarks">
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
    <keyboard-shortcut first-keystroke="ctrl k" second-keystroke="ctrl plus" />
  </action>
  <action id="SplitHorizontally">
    <keyboard-shortcut first-keystroke="ctrl #10000ba" second-keystroke="ctrl down" />
  </action>
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="ctrl #10000ba" second-keystroke="ctrl #10000ba" />
  </action>
</keymap>